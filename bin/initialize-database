function bulkCreate(tweets) {
    Tweet.bulkCreate(tweets)
        .then(() => {
            return Tweet.findAll();
        })
        .then(tweets => console.log("inserted tweets: ", tweets));
}

(function () {
    console.log("Initializing database");
    bulkCreate(test);
})();


